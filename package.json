{
  "name": "angular-tour-of-heroes",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:dynamic": "npm run build:dynamic && npm run serve:dynamic",
    "start:static": "npm run build:static && npm run serve:static",
    "build:client-and-server-bundles": "ng build --prod --aot && ng build --prod --app 1 --output-hashing=false --aot",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "build:dynamic": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:static": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:static": "cd dist/browser && http-server",
    "serve:dynamic": "node dist/server",
    "postinstall": "bower install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.1",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "5.0.1",
    "@angular/compiler": "5.0.1",
    "@angular/core": "5.0.1",
    "@angular/forms": "5.0.1",
    "@angular/http": "5.0.1",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser-dynamic": "5.0.1",
    "@angular/platform-browser": "5.0.1",
    "@angular/platform-server": "5.0.1",
    "@angular/router": "5.0.1",
    "@codebakery/origami": "1.3.2",
    "@nguniversal/express-engine": "5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0-beta.5",
    "angular-in-memory-web-api": "0.5.1",
    "core-js": "2.5.1",
    "immutable": "3.8.2",
    "jsdom": "11.3.0",
    "ramda": "0.25.0",
    "rxjs": "5.5.2",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "5.0.1",
    "@angular/language-service": "5.0.1",
    "@types/jasmine": "2.6.3",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.0.51",
    "codelyzer": "4.0.1",
    "cpy-cli": "1.0.1",
    "http-server": "0.10.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jasmine": "1.1.0",
    "karma": "1.7.1",
    "protractor": "5.2.0",
    "reflect-metadata": "0.1.10",
    "ts-loader": "3.1.1",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.6.1"
  }
}
